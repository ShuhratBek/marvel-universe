{
  "name": "marvel-universe",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Shukhratbek",
  "scripts": {
    "lint": "standard ./src",
    "test": "npm run test:unit && npm run test:int",
    "test:unit": "npm run mocha ./src/**/*.unit.js",
    "test:int": "npm run mocha ./src/**/*.int.js --exit",
    "test:cov": "nyc npm run test",
    "mocha": "cross-env NODE_ENV=test mocha --require babel-register --recursive",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run -s build",
    "start": "node dist/index.js",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "dev": "cross-env NODE_ENV=development DEBUG=marvel-universe nodemon --exec babel-node ./src/index.js"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-validation": "^1.0.2",
    "express-winston": "^2.4.0",
    "helmet": "^3.10.0",
    "http-status": "^1.0.1",
    "joi": "^13.1.1",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^2.3.10",
    "mkdir": "0.0.2",
    "mongoose": "^5.0.2",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.14.12",
    "rimraf": "^2.6.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "faker": "^4.1.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "standard": "^10.0.3",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  }
}
